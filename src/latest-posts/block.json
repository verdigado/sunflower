{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "sunflower/latest-posts",
	"version": "2.1.0",
	"title": "Latest Posts (Sunflower)",
	"category": "sunflower-blocks",
	"icon": "admin-post",
	"description": "Show Latest Posts in Sunflower Design",
	"example": {},
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"archiveText": {
			"type": "string",
			"default": ""
		},
		"categories": {
			"type": "array"
		},
		"excludedCategories": {
			"type": "array"
		},
		"count": {
			"type": "number",
			"default": 6
		},
		"blockLayout": {
			"type": "string",
			"default": "grid"
		},
		"columns": {
			"type": "number",
			"default": 2
		},
		"excerptLength": {
			"type": "number",
			"default": 55
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "sunflower-latest-posts",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}
