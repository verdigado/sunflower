{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"useRootPaddingAwareAlignments": false,


		"color": {

			"defaultPalette": false,
			"custom": false,
			"defaultGradients": false,
			"customGradient": false,
			"defaultDuotone": false,
			"customDuotone": false,

			"palette": [
				{
					"color": "#002216",
					"name": "Schwarzwald",
					"slug": "black"
				},
				{
					"color": "#005538",
					"name": "Tanne",
					"slug": "tanne"
				},
				{
					"color": "#008939",
					"name": "Klee",
					"slug": "klee"
				},
				{
					"color": "#8ABD24",
					"name": "Grashalm",
					"slug": "grashalm"
				},
				{
					"color": "#FFF17A",
					"name": "Sonne",
					"slug": "sonne"
				},
				{
					"color": "#F5F1E9",
					"name": "Sand",
					"slug": "sand"
				},
				{
					"color": "#374951",
					"name": "Maus",
					"slug": "gray"
				},
				{
					"color": "#e8eae9",
					"name": "Kalk",
					"slug": "kalk"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#0BA1DD",
					"name": "Himmel",
					"slug": "himmel"
				}
			]
		},
		"layout": {
			"contentSize": "900px",
			"wideSize": "1200px",
			"allowCustomContentAndWideSize": true
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-regular.woff2",
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-regular.ttf"
							]
						},
						{
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-italic.woff2",
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-italic.ttf"
							]
						},
						{
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-700.woff2",
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-700.ttf"
							]
						},
						{
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-700italic.woff2",
								"file:./assets/fonts/pt-sans-v17-latin_latin-ext-700italic.ttf"
							]
						}
					],
					"fontFamily": "\"PT Sans\", sans-serif",
					"name": "PT Sans",
					"slug": "pt-sans"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "GrueneType Neue",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/GrueneTypeNeue-Regular.woff2",
								"file:./assets/fonts/GrueneTypeNeue-Regular.ttf"
							]
						}
					],
					"fontFamily": "\"GruenType Neue\", sans-serif",
					"name": "GrueneType Neue",
					"slug": "gruenetypeneue"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|klee",
					"text": "var:preset|color|white"
				}
			},
			"core/heading": {
				"typography": {
					"fontWeight": "400"
				}
			}
		},
		"typography": {
			"fontWeight": "400",
			"fontFamily": "var(--wp--preset--font-family--pt-sans)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontWeight": "400",
					"fontFamily": "var(--wp--preset--font-family--gruenetypeneue)"
				},
				"color": {
					"text": "var:preset|color|tanne"
				}
			}
		}
	}
}
